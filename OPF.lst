 148  * ------------------------------------------------------------------------
      -----
 149  * PARAMETER DEFINITION
 150   
 151  Parameters
 152   
 153  R          (i,j)
 154  CCSref     (i,j)
 155  CCScost    (i,j)
 156  CCSmax     (i,j)
 157  CCSmin     (i,j)
 158  CCLoads    (i,j)
 159  ;
 160   
 161  R        (i,j)  =  Rmap(i,j,'Rline')           ;
 162  CCSref   (i,j)  =  CCsourcesmap(i,j,'CCSmap') ;
 163  CCScost  (i,j)  =  CCsourcesmap(i,j,'CCScost') ;
 164  CCSmax   (i,j)  =  CCsourcesmap(i,j,'CCSmax')  ;
 165  CCSmin   (i,j)  =  CCsourcesmap(i,j,'CCSmin')  ;
 166  CCLoads  (i,j)  =  CCloadsmap(i,j,'CCLmap')  ;
 167   
 168   
 169  Scalar
 170  Vnom       /47.5/
 171  ;
 172   
 173  * ------------------------------------------------------------------------
      -----
 174  * DYNAMIC SET OF INDICES
 175   
 176  ij    (i,j)    $[R(i,j)]          = yes;
 177  CCs   (i,j)    $[CCSref(i,j)]     = yes;
 178   
 179  * ------------------------------------------------------------------------
      -----
 180  * VARIABLE DEFINITION
 181   
 182  Variables
 183  Ibr       (i,j)         'Branch Current'
 184  In        (i)           'Nodal Current'
 185  V         (i)           'Nodal Voltage'
 186  CCSources (i,j)         'Constant Current Source'
 187  cost
 188   
 189  * ------------------------------------------------------------------------
      -----
 190  * POWER FLOW EQUATIONS
 191   
 192  Equations
 193   
 194  AllCurrentSources (i)     'Sum of currents of all sources at node i'
 195  BranchCurrent     (i,j)   'Current in branch ij'
 196  NodalCurrent      (i)     'Nodal current equation'
 197  Costfunction              'Cost function to be minimized'
 198  ;
 199   
 200  * ------------------------------------------------------------------------
      -----
 201  * Set Voltage limits and fixed nodal voltages
 202  V.lo(i)     =   -200;
 203  V.up(i)     =    200;
 204  V.fx('n1')  =    0;
 205   
 206  * ------------------------------------------------------------------------
      -----
 207  * Circuit Equation
 208   
 209  BranchCurrent     (ij(i,j)) .. Ibr(i,j) =e=  (R(i,j)*(V(i)-V(j)));
 210  NodalCurrent      (i)       .. In(i)    =e=  sum[j $ij(i,j)   ,Ibr(i,j)] -
       sum[j $ij(j,i), Ibr(j,i)];
 211  AllCurrentSources (i)       .. In(i)    =e=  sum[j $CCs(i,j)  ,CCSources(i
      ,j)];
 212   
 213  * ------------------------------------------------------------------------
      -----
 214  * OPTIMAL POWER FLOW EQUATIONS
 215   
 216  Costfunction                ..  cost    =e=  sum[CCs(i,j), CCSources(i,j)*
      CCScost(i,j)];
 217   
 218  model OPF /all/;
 219   
 220  solve OPF minimizing cost using lp;
